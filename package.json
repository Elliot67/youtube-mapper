{
	"name": "youtube-mapper",
	"version": "1.0.0",
	"author": "Elliot67",
	"license": "MIT",
	"scripts": {
		"b-watch": "tsc --project ./src/back/tsconfig.json -w",
		"b-build": "tsc --project ./src/back/tsconfig.json",
		"b-start": "electron ./src/back/dist/main.js --inspect --trace-warnings",
		"f-watch": "parcel watch ./src/front/index.html --out-dir ./src/front/dist --public-url ./ --no-hmr",
		"f-build": "parcel build ./src/front/index.html --out-dir ./src/front/dist --public-url ./"
	},
	"devDependencies": {
		"@babel/core": "^7.14.5",
		"@types/d3": "^7.0.0",
		"@types/dagre": "^0.7.44",
		"@types/dagre-d3": "^0.6.2",
		"electron": "^13.1.1",
		"parcel-bundler": "^1.12.5",
		"sass": "^1.34.1",
		"typescript": "^4.3.4"
	},
	"dependencies": {
		"d3": "^7.0.0",
		"dagre-d3": "^0.6.4",
		"ytdl-core": "^4.8.3"
	},
	"browserslist": [
		"last 1 Chrome version"
	]
}
