{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Main Process",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
			"args": [
				"./src/back/dist/main.js",
				"--inspect",
				"--trace-warnings"
			],
			"outputCapture": "std",
			"env": {
				"NODE_ENV": "dev"
			}
		}
	]
}
